<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.FlowPane?>
<VBox
    fx:controller="pl.yellowduck.netflix90.gui.MainController"
    prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Application">
          <items>
            <MenuItem mnemonicParsing="false" text="Quit" onAction="#quit"/>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Films" onAction="#films">
        </Menu>
        <Menu mnemonicParsing="false" text="Dictionaries">
          <items>
            <MenuItem mnemonicParsing="false" text="New category" onAction="#newCategory"/>
            <MenuItem mnemonicParsing="false" text="Categories" onAction="#categories"/>
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <FlowPane fx:id="mainPanel" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
    </FlowPane>
  </children>
</VBox>
